{
  "name": "xelis-explorer",
  "version": "0.2.0",
  "description": "Official Explorer for XELIS Blockchain.",
  "scripts": {
    "make-lang": "node ./node_modules/g45-react/lang-dict.js --src ./src --outfile ./lang/en.json",
    "bundle": "shx cp -n ./env/local_dev.json ./bundler-define.json && node ./node_modules/g45-react/bundler.js --entry ./src/routes.js",
    "build-dev:index": "npm run bundle -- --type index --watch --sourcemap",
    "build-dev:cf": "npm run bundle -- --type cf_worker --watch --sourcemap",
    "start-dev:cf": "npx wrangler pages dev ./dist/cf --port=3000 --live-reload",
    "build-dev:node": "npm run bundle -- --type node_server --watch --sourcemap",
    "start-dev:node": "node-dev ./dist/node_server/server.js --notify=false",
    "build-prod:index": "npm run bundle -- --type index --minify",
    "build-prod:node": "npm run bundle -- --type node_server --minify",
    "build-prod:cf": "npm run bundle -- --type cf_worker --minify",
    "build-cf:dev-pages": "npm run build-dev:cf -- --define ./env/cf_dev.json",
    "build-cf:testnet-pages": "npm run build-prod:cf -- --define ./env/cf_testnet.json",
    "build-cf:mainnet-pages": "npm run build-prod:cf -- --define ./env/cf_mainnet.json",
    "build": "npm run build-dev:cf",
    "start": "npm run start-dev:cf"
  },
  "dependencies": {
    "@emeraldpay/hashicon": "^0.5.2",
    "@react-three/drei": "^9.57.0",
    "@react-three/fiber": "^8.11.9",
    "@tweenjs/tween.js": "^21.0.0",
    "@xelis/sdk": "^0.8.1",
    "await-to-js": "^3.0.0",
    "bignumber.js": "^9.1.2",
    "bytes": "^3.1.2",
    "chart.js": "^4.2.1",
    "flag-icons": "^6.11.2",
    "framer-motion-3d": "^10.1.0",
    "goober": "^2.1.10",
    "hash-it": "^6.0.0",
    "isomorphic-ws": "^5.0.0",
    "leaflet": "github:g45t345rt/Leaflet",
    "leaflet.markercluster": "github:g45t345rt/leaflet.markercluster",
    "pretty-ms": "^8.0.0",
    "qrcode.react": "^3.1.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.7.0",
    "reset-css": "^5.0.1",
    "store2": "^2.14.2",
    "three": "^0.150.1"
  },
  "devDependencies": {
    "g45-react": "^0.1.9",
    "node-dev": "^8.0.0",
    "shx": "^0.3.4",
    "wrangler": "^3.37.0"
  }
}
